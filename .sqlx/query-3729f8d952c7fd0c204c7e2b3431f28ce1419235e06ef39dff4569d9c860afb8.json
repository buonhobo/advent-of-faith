  "query": "\n            SELECT unlocked_at, unlocks_at, cd.calendar_id, cd.id as day_id\n            FROM calendar_days as cd\n            JOIN calendars as c ON cd.calendar_id = c.id\n            LEFT JOIN user_days as ud ON cd.id = ud.day_id\n            WHERE calendar_id = ANY($1)\n            ORDER BY unlocks_at\n            ",
