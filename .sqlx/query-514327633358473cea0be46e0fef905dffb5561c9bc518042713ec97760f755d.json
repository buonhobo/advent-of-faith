  "query": "\n            WITH inserted AS (\n                INSERT INTO user_sessions (user_id) VALUES ($1)\n                RETURNING id, user_id, created_at, expires_at)\n            SELECT\n                u.id AS user_id, u.username, u.role AS \"role:_\",\n                s.id, s.created_at, s.expires_at\n            FROM inserted AS s\n            JOIN users AS u ON u.id = s.user_id\n            ",
